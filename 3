class Solution:
    def rotateString(self, s: str, goal: str) -> bool:
        if len(s) != len(goal):
           return False
        length=len(s)
        for i in range(length):
            s=s[1:]+s[0]
            if s==goal:
                return True
        return False
if __name__ == "__main__":
    solution=Solution()
    s="abcde"
    goal="eabcd" #True
    goal="acdeb" #False
    bool=solution.rotateString(s,goal)
    print(bool)
                
