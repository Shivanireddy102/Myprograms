from typing import List
class Solution:
    def largestCombination(self, candidates: List[int]) -> int:
        bit_count = [0] * 24
        for i in range(24):
            for nums in candidates:
                if (nums & (1 << i)) != 0:
                    bit_count[i] += 1
        return max(bit_count)
solution = Solution()
candidates = [16, 17, 71, 62, 12, 24, 14]
print(solution.largestCombination(candidates)) 
