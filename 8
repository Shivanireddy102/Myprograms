from typing import List
class Solution:
    def getMaximumXor(self, nums: List[int], maximumBit: int) -> List[int]:
        xor_product = 0
        for num in nums:
            xor_product ^= num
        ans = [0] * len(nums)
        mask = (1 << maximumBit) - 1
        for i in range(len(nums)):
            ans[i] = xor_product ^ mask
            xor_product ^= nums[len(nums) - 1 - i]
        return ans
solution = Solution()
nums = [0,1,1,3]
print(solution.getMaximumXor(nums, 2))
